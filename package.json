{
  "name": "typing-monitor",
  "version": "0.1.0",
  "description": "Keyboard typing detection made easy",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src/ -d dist/",
    "lint": "eslint src test build",
    "build:watch": "babel -w src/ -d dist/",
    "flow": "flow",
    "start": "babel-node -w --presets flow",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch --verbose"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsmd/typing-monitor.git"
  },
  "keywords": [
    "keyboard",
    "input",
    "javascript",
    "events"
  ],
  "author": "Waseem Dahman <dwaseem@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wsmd/typing-monitor/issues"
  },
  "homepage": "https://github.com/wsmd/typing-monitor#readme",
  "devDependencies": {
    "@types/jest": "21.1.1",
    "babel-cli": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "21.2.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "6.23.0",
    "eslint": "4.11.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-airbnb-flow": "1.0.2",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jest": "21.3.2",
    "flow-bin": "0.59.0",
    "jest": "21.2.1",
    "regenerator-runtime": "0.11.0"
  },
  "jest": {
    "testRegex": "(/test/.*.test.js)$"
  }
}
