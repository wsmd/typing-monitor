{
  "extends": [
    "airbnb-base",
    "plugin:flowtype/recommended"
  ],
  "env": {
    "jest": true,
    "browser": true
  },
  "plugins": [
    "flowtype",
    "jest"
  ],
  "rules": {
    "import/prefer-default-export": "off",
    "function-paren-newline": "off",
    "comma-dangle": ["error", "always-multiline"]
  }
}
